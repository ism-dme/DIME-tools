                                <field name="unparsedInsertString">
                                    <String>&amp;lt;${selection}&amp;gt;${caret}</String>
                                </field>
                                <field name="descriptionString">
                                    <String>Inserts angular brackets (&lt;>) as XML-entities. You may also wrap any element (triple-click the element before the applying). Shortcut: "Ctrl+Shift-Period"</String>
                                </field>
                                <field name="renderString">
                                    <String>angular brackets</String>
                                </field>
                                <field name="enabled">
                                    <Boolean>true</Boolean>
                                </field>
                                <field name="accelerator">

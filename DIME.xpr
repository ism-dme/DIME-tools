                                </field>
                            </codeTemplateItem>
                            <codeTemplateItem>
                                <field name="contentType">
                                    <String>text/any</String>
                                </field>
                                <field name="unparsedInsertString">
                                    <String>&lt;choice xml:id="choice_${id}">
  &lt;reg resp="${ask('@resp?', editable_combobox, ('#DME-editors':'#DME-editors'), '#DME-editors')}" type="${ask('@type?', editable_combobox, ('accid_mod':'accid_mod';'alt_notation':'alt_notation';'beam_mod':'beam_mod';'alt_representation':'alt_representation';'editorial_normalization':'editorial_normalization';'structural_repetition':'structural_repetition';'positioning':'positioning';'alt_representation':'alt_representation';'doubling':'doubling';'positioning':'positioning';'shorthand':'shorthand';'shorthand_slur':'shorthand_slur';'editoral_normalization':'editoral_normalization'), '#editorial_normalization')}" xml:id="reg_${id}">
    ${xpath_eval(    
    let $node := parse-xml-fragment('${selection}')/node(),
    $string := serialize($node),
    $pattern := '(&lt;.*?xml:id=")(.*?)(".*?>)',
      $replacement := '$1$2a$3'
      return
      replace($string, $pattern,$replacement)
      )}
  &lt;/reg>
  &lt;orig xml:id="orig_${id}">
    ${xpath_eval(    
    let $node := parse-xml-fragment('${selection}')/node(),
    $string := serialize($node),
    $pattern := '(&lt;.*?xml:id=")(.*?)(".*?>)',
      $replacement := '$1$2b$3'
      return
      replace($string, $pattern,$replacement)
      )}${caret} 
  &lt;/orig>
&lt;/choice></String>

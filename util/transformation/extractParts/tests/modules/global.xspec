<?xml version="1.0" encoding="UTF-8"?>
<x:description run-as="external" stylesheet="../../main.xsl" xmlns:test-helper="x-urn:tutorial:helper:ws-only-text:test-helper" xmlns:dme="http://www.mozarteum.at/ns/dme" xmlns:mei="http://www.music-encoding.org/ns/mei" xmlns:x="http://www.jenitennison.com/xslt/xspec">

  <x:helper stylesheet="../../../lib/test-helper.xsl"/>

  <x:scenario focus="on" label="Tests for global templates and functions">

    <x:scenario label="When all available parts are selected for extraction, the file should be just copied.">
      <x:scenario label="Extract Oboi (|2|)">

        <x:variable href="../data/beam-0.mei" name="input.file.01" select="test-helper:remove-whitespace-only-text(.)"/>
        <x:param name="parts">|2|</x:param>
        <x:context select="$input.file.01"/>
        <x:call template="main"/>
        <x:expect select="$input.file.01" label="File is copied."/>
      </x:scenario>

      <x:scenario label="Extract all avaialable parts (|1|2|3|4|5|6|7|8|9|)">
        <x:param name="parts">|1|2|3|4|5|6|7|8|9|</x:param>
        <x:variable href="../data/dmeref_550-003_mm_8-21.mei" name="input.file.02" select="test-helper:remove-whitespace-only-text(.)"/>
        <x:context select="$input.file.02"/>
        <x:call template="main"/>
        <x:expect select="$input.file.02" label="File is copied."/>
      </x:scenario>
    </x:scenario>
    <!--    $input.file.02/mei:mei-->
  </x:scenario>












</x:description>
